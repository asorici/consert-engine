  <owl:Ontology rdf:about="">
    <spin:imports rdf:resource="http://spinrdf.org/spl.ttl"/>
    <owl:imports rdf:resource="http://pervasive.semanticweb.org/ont/2013/05/contextassertion"/>
    <spin:imports rdf:resource="http://pervasive.semanticweb.org/ont/2013/05/contextassertion/functions"/>
    <owl:imports rdf:resource="http://pervasive.semanticweb.org/ont/2004/06/action"/>
    <owl:imports rdf:resource="http://pervasive.semanticweb.org/ont/2004/06/meeting"/>
