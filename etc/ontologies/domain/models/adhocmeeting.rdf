  <spin:RuleProperty rdf:about="http://spinrdf.org/spin#deriveassertion">
    <rdfs:range rdf:resource="http://spinrdf.org/spin#Rule"/>
    <rdfs:domain rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdfs:subPropertyOf rdf:resource="http://spinrdf.org/spin#rule"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >deriveassertion</rdfs:label>
  </spin:RuleProperty>
  <rdf:Description rdf:about="http://pervasive.semanticweb.org/ont/2013/05/contextassertion#dynamicRelationAssertion">
    <spin:abstract rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean"
