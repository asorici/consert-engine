  <owl:Class rdf:ID="Microphone">
    <rdfs:subClassOf>
      <rdf:Description rdf:about="http://pervasive.semanticweb.org/ont/2004/06/device#Device">
        <owl:hasKey rdf:parseType="Collection">
          <rdf:Description rdf:about="http://pervasive.semanticweb.org/ont/2004/06/device#modelNumber"/>
          <rdf:Description rdf:about="http://pervasive.semanticweb.org/ont/2004/06/device#serialNumber"/>
        </owl:hasKey>
      </rdf:Description>
    </rdfs:subClassOf>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Microphone</rdfs:label>
  </owl:Class>
  <owl:Class rdf:ID="BuildingFloor">
    <owl:disjointWith>
      <owl:Class rdf:ID="BuildingRoom"/>
    </owl:disjointWith>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >BuildingFloor</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://pervasive.semanticweb.org/ont/2004/06/space#SpaceInAFixedStructure"/>
    <owl:disjointWith>
      <owl:Class rdf:ID="BuildingCorp"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:ID="BuildingRoomSection"/>
    </owl:disjointWith>
  </owl:Class>
  <owl:Class rdf:ID="Skeleton">
    <owl:hasKey rdf:parseType="Collection">
      <owl:DatatypeProperty rdf:ID="hasTrackingID"/>
    </owl:hasKey>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Skeleton</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://pervasive.semanticweb.org/ont/2013/05/contextassertion#ContextEntity"/>
  </owl:Class>
  <owl:Class rdf:ID="HostsAdHocMeeting">
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:allValuesFrom>
          <owl:Class rdf:about="#BuildingRoomSection"/>
        </owl:allValuesFrom>
        <owl:onProperty>
          <owl:FunctionalProperty rdf:about="http://pervasive.semanticweb.org/ont/2013/05/contextassertion#assertionRole"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:hasValue rdf:resource="http://pervasive.semanticweb.org/ont/2013/05/contextassertion#Derived"/>
        <owl:onProperty>
          <owl:FunctionalProperty rdf:about="http://pervasive.semanticweb.org/ont/2013/05/contextassertion#assertionType"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >HostsAdHocMeeting</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://pervasive.semanticweb.org/ont/2013/05/contextassertion#UnaryContextAssertion"/>
  </owl:Class>
  <owl:Class rdf:about="#BuildingRoom">
    <owl:disjointWith rdf:resource="#BuildingFloor"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >BuildingRoom</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://pervasive.semanticweb.org/ont/2004/06/space#SpaceInAFixedStructure"/>
    <owl:disjointWith>
      <owl:Class rdf:about="#BuildingCorp"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:about="#BuildingRoomSection"/>
    </owl:disjointWith>
  </owl:Class>
  <owl:Class rdf:about="#BuildingRoomSection">
    <owl:disjointWith>
      <owl:Class rdf:about="#BuildingCorp"/>
    </owl:disjointWith>
    <owl:disjointWith rdf:resource="#BuildingRoom"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >BuildingRoomSection</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://pervasive.semanticweb.org/ont/2004/06/space#SpaceInAFixedStructure"/>
    <owl:disjointWith rdf:resource="#BuildingFloor"/>
    <spin:deriveassertion>
      <hostsAdHocMeetingContstruct/>
    </spin:deriveassertion>
    <spin:thisUnbound rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean"
    >true</spin:thisUnbound>
  </owl:Class>
  <owl:Class rdf:ID="Building">
    <rdfs:subClassOf rdf:resource="http://pervasive.semanticweb.org/ont/2004/06/space#FixedStructure"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Building</rdfs:label>
  </owl:Class>
  <owl:Class rdf:ID="KinectCamera">
    <rdfs:subClassOf rdf:resource="http://pervasive.semanticweb.org/ont/2004/06/device#Device"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >KinectCamera</rdfs:label>
  </owl:Class>
  <owl:Class rdf:ID="SkeletonPosition">
    <owl:oneOf rdf:parseType="Collection">
      <SkeletonPosition rdf:ID="crouching">
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
        >SkeletonPosition</rdfs:label>
      </SkeletonPosition>
      <SkeletonPosition rdf:ID="leaning">
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
        >SkeletonPosition</rdfs:label>
      </SkeletonPosition>
      <SkeletonPosition rdf:ID="sitting">
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
        >SkeletonPosition</rdfs:label>
      </SkeletonPosition>
      <SkeletonPosition rdf:ID="standing">
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
        >SkeletonPosition</rdfs:label>
      </SkeletonPosition>
      <SkeletonPosition rdf:ID="walking">
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
        >SkeletonPosition</rdfs:label>
      </SkeletonPosition>
    </owl:oneOf>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >SkeletonPosition</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://pervasive.semanticweb.org/ont/2013/05/contextassertion#ContextEntity"/>
  </owl:Class>
  <owl:Class rdf:about="#BuildingCorp">
    <owl:disjointWith rdf:resource="#BuildingRoom"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >BuildingCorp</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://pervasive.semanticweb.org/ont/2004/06/space#SpaceInAFixedStructure"/>
    <owl:disjointWith rdf:resource="#BuildingRoomSection"/>
    <owl:disjointWith rdf:resource="#BuildingFloor"/>
  </owl:Class>
  <owl:Class rdf:ID="SensesSkelInPosition">
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:allValuesFrom rdf:resource="#SkeletonPosition"/>
        <owl:onProperty>
          <owl:ObjectProperty rdf:ID="skelPositionRole"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:allValuesFrom rdf:resource="#Skeleton"/>
        <owl:onProperty>
          <owl:ObjectProperty rdf:ID="skeletonRole"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:allValuesFrom rdf:resource="#KinectCamera"/>
        <owl:onProperty>
          <owl:ObjectProperty rdf:ID="kinectCameraRole"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <owl:hasKey rdf:parseType="Collection">
      <owl:ObjectProperty rdf:about="#kinectCameraRole"/>
      <owl:ObjectProperty rdf:about="#skeletonRole"/>
    </owl:hasKey>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="http://pervasive.semanticweb.org/ont/2013/05/contextassertion#assertionType"/>
        <owl:hasValue rdf:resource="http://pervasive.semanticweb.org/ont/2013/05/contextassertion#Sensed"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >SensesSkelInPosition</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://pervasive.semanticweb.org/ont/2013/05/contextassertion#NaryContextAssertion"/>
  </owl:Class>
  <owl:ObjectProperty rdf:about="#skelPositionRole">
    <rdfs:subPropertyOf rdf:resource="http://pervasive.semanticweb.org/ont/2013/05/contextassertion#assertionRole"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >skelPositionRole</rdfs:label>
    <rdfs:range rdf:resource="#SkeletonPosition"/>
    <rdfs:domain rdf:resource="#SensesSkelInPosition"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#kinectCameraRole">
    <rdfs:domain rdf:resource="#SensesSkelInPosition"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >SkeletonPosition</rdfs:label>
    <rdfs:subPropertyOf rdf:resource="http://pervasive.semanticweb.org/ont/2013/05/contextassertion#assertionRole"/>
    <rdfs:range rdf:resource="#KinectCamera"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="personRoomSection">
    <rdfs:subPropertyOf>
      <rdf:Description rdf:about="http://pervasive.semanticweb.org/ont/2013/05/contextassertion#sensedRelationAssertion">
        <spin:abstract rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean"
        >true</spin:abstract>
      </rdf:Description>
    </rdfs:subPropertyOf>
    <rdfs:domain>
      <rdf:Description rdf:about="http://pervasive.semanticweb.org/ont/2004/06/person#Person">
        <spin:constraint>
          <sp:Ask>
            <sp:where rdf:parseType="Collection">
              <sp:NamedGraph>
                <sp:graphNameNode rdf:parseType="Resource">
                  <sp:varName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                  >g1</sp:varName>
                </sp:graphNameNode>
                <sp:elements rdf:parseType="Collection">
                  <rdf:Description>
                    <sp:subject rdf:parseType="Resource">
                      <sp:varName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                      >P</sp:varName>
                    </sp:subject>
                    <sp:predicate rdf:resource="#personRoomSection"/>
                    <sp:object rdf:parseType="Resource">
                      <sp:varName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                      >RoomLoc1</sp:varName>
                    </sp:object>
                  </rdf:Description>
                </sp:elements>
              </sp:NamedGraph>
              <sp:NamedGraph>
                <sp:graphNameNode rdf:resource="http://pervasive.semanticweb.org/ont/2013/09/adhocmeeting/store#personRoomSectionStore"/>
                <sp:elements rdf:parseType="Collection">
                  <rdf:Description>
                    <sp:subject rdf:parseType="Resource">
                      <sp:varName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                      >g1</sp:varName>
                    </sp:subject>
                    <sp:predicate rdf:resource="http://pervasive.semanticweb.org/ont/2013/05/contextassertion#validDuring"/>
                    <sp:object rdf:parseType="Resource">
                      <sp:varName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                      >validity</sp:varName>
                    </sp:object>
                  </rdf:Description>
                  <rdf:Description>
                    <sp:subject rdf:parseType="Resource">
                      <sp:varName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                      >g1</sp:varName>
                    </sp:subject>
                    <sp:predicate rdf:resource="http://pervasive.semanticweb.org/ont/2013/05/contextassertion#hasAccuracy"/>
                    <sp:object rdf:parseType="Resource">
                      <sp:varName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                      >acc1</sp:varName>
                    </sp:object>
                  </rdf:Description>
                </sp:elements>
              </sp:NamedGraph>
              <sp:Filter>
                <sp:expression>
                  <sp:and>
                    <sp:arg1>
                      <sp:ge>
                        <sp:arg1 rdf:parseType="Resource">
                          <sp:varName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                          >acc</sp:varName>
                        </sp:arg1>
                        <sp:arg2 rdf:datatype="http://www.w3.org/2001/XMLSchema#decimal"
                        >0.75</sp:arg2>
                      </sp:ge>
                    </sp:arg1>
                    <sp:arg2>
                      <sp:exists>
                        <sp:elements rdf:parseType="Collection">
                          <sp:NamedGraph>
                            <sp:graphNameNode rdf:parseType="Resource">
                              <sp:varName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                              >g2</sp:varName>
                            </sp:graphNameNode>
                            <sp:elements rdf:parseType="Collection">
                              <rdf:Description>
                                <sp:subject rdf:parseType="Resource">
                                  <sp:varName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                                  >P</sp:varName>
                                </sp:subject>
                                <sp:predicate rdf:resource="#personRoomSection"/>
                                <sp:object rdf:parseType="Resource">
                                  <sp:varName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                                  >RoomLoc2</sp:varName>
                                </sp:object>
                              </rdf:Description>
                            </sp:elements>
                          </sp:NamedGraph>
                          <sp:NamedGraph>
                            <sp:graphNameNode rdf:resource="http://pervasive.semanticweb.org/ont/2013/09/adhocmeeting/store#personRoomSectionStore"/>
                            <sp:elements rdf:parseType="Collection">
                              <rdf:Description>
                                <sp:subject rdf:parseType="Resource">
                                  <sp:varName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                                  >g2</sp:varName>
                                </sp:subject>
                                <sp:predicate rdf:resource="http://pervasive.semanticweb.org/ont/2013/05/contextassertion#validDuring"/>
                                <sp:object rdf:parseType="Resource">
                                  <sp:varName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                                  >validity2</sp:varName>
                                </sp:object>
                              </rdf:Description>
                              <rdf:Description>
                                <sp:subject rdf:parseType="Resource">
                                  <sp:varName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                                  >g2</sp:varName>
                                </sp:subject>
                                <sp:predicate rdf:resource="http://pervasive.semanticweb.org/ont/2013/05/contextassertion#hasAccuracy"/>
                                <sp:object rdf:parseType="Resource">
                                  <sp:varName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                                  >acc2</sp:varName>
                                </sp:object>
                              </rdf:Description>
                            </sp:elements>
                          </sp:NamedGraph>
                          <sp:Filter>
                            <sp:expression>
                              <sp:and>
                                <sp:arg1>
                                  <sp:and>
                                    <sp:arg1>
                                      <sp:and>
                                        <sp:arg1>
                                          <sp:ne>
                                            <sp:arg1 rdf:parseType="Resource">
                                              <sp:varName rdf:datatype=
                                              "http://www.w3.org/2001/XMLSchema#string"
                                              >RoomLoc1</sp:varName>
                                            </sp:arg1>
                                            <sp:arg2 rdf:parseType="Resource">
                                              <sp:varName rdf:datatype=
                                              "http://www.w3.org/2001/XMLSchema#string"
                                              >RoomLoc2</sp:varName>
                                            </sp:arg2>
                                          </sp:ne>
                                        </sp:arg1>
                                        <sp:arg2>
                                          <sp:ge>
                                            <sp:arg1 rdf:parseType="Resource">
                                              <sp:varName rdf:datatype=
                                              "http://www.w3.org/2001/XMLSchema#string"
                                              >acc2</sp:varName>
                                            </sp:arg1>
                                            <sp:arg2 rdf:datatype=
                                            "http://www.w3.org/2001/XMLSchema#decimal"
                                            >0.75</sp:arg2>
                                          </sp:ge>
                                        </sp:arg2>
                                      </sp:and>
                                    </sp:arg1>
                                    <sp:arg2>
                                      <sp:lt>
                                        <sp:arg1 rdf:parseType="Resource">
                                          <sp:varName rdf:datatype=
                                          "http://www.w3.org/2001/XMLSchema#string"
                                          >acc1</sp:varName>
                                        </sp:arg1>
                                        <sp:arg2 rdf:parseType="Resource">
                                          <sp:varName rdf:datatype=
                                          "http://www.w3.org/2001/XMLSchema#string"
                                          >acc2</sp:varName>
                                        </sp:arg2>
                                      </sp:lt>
                                    </sp:arg2>
                                  </sp:and>
                                </sp:arg1>
                                <sp:arg2>
                                  <cfn:validityIntervalsOverlap>
                                    <sp:arg1 rdf:parseType="Resource">
                                      <sp:varName rdf:datatype=
                                      "http://www.w3.org/2001/XMLSchema#string"
                                      >validity</sp:varName>
                                    </sp:arg1>
                                    <sp:arg2 rdf:parseType="Resource">
                                      <sp:varName rdf:datatype=
                                      "http://www.w3.org/2001/XMLSchema#string"
                                      >validity2</sp:varName>
                                    </sp:arg2>
                                  </cfn:validityIntervalsOverlap>
                                </sp:arg2>
                              </sp:and>
                            </sp:expression>
                          </sp:Filter>
                        </sp:elements>
                      </sp:exists>
                    </sp:arg2>
                  </sp:and>
                </sp:expression>
              </sp:Filter>
            </sp:where>
          </sp:Ask>
        </spin:constraint>
      </rdf:Description>
    </rdfs:domain>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >personRoomSection</rdfs:label>
    <rdfs:range rdf:resource="#BuildingRoomSection"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="roomSectionRole">
    <rdfs:range rdf:resource="#BuildingRoomSection"/>
    <rdfs:domain rdf:resource="#HostsAdHocMeeting"/>
    <rdfs:subPropertyOf rdf:resource="http://pervasive.semanticweb.org/ont/2013/05/contextassertion#assertionRole"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >roomSectionRole</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#skeletonRole">
    <rdfs:domain rdf:resource="#SensesSkelInPosition"/>
    <rdfs:subPropertyOf rdf:resource="http://pervasive.semanticweb.org/ont/2013/05/contextassertion#assertionRole"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >skeletonRole</rdfs:label>
    <rdfs:range rdf:resource="#Skeleton"/>
  </owl:ObjectProperty>
  <rdf:Property rdf:about="http://spinrdf.org/arg#graphUUID">
    <rdfs:subPropertyOf rdf:resource="http://spinrdf.org/sp#arg"/>
  </rdf:Property>
  <rdf:Property rdf:about="http://spinrdf.org/arg#intervalSet1">
    <rdfs:subPropertyOf rdf:resource="http://spinrdf.org/sp#arg"/>
  </rdf:Property>
  <owl:DatatypeProperty rdf:about="#hasTrackingID">
    <rdfs:subPropertyOf rdf:resource="http://pervasive.semanticweb.org/ont/2013/05/contextassertion#entityDataDescription"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >hasTrackingID</rdfs:label>
  </owl:DatatypeProperty>
  <owl:FunctionalProperty rdf:ID="hasNoiseLevel">
    <rdfs:domain rdf:resource="#Microphone"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#double"/>
    <rdfs:subPropertyOf rdf:resource="http://pervasive.semanticweb.org/ont/2013/05/contextassertion#sensedDataAssertion"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >hasNoiseLevel</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:ID="deviceRoomSection">
    <rdfs:range rdf:resource="#BuildingRoomSection"/>
    <rdfs:subPropertyOf>
      <rdf:Description rdf:about="http://pervasive.semanticweb.org/ont/2013/05/contextassertion#profiledRelationAssertion">
        <spin:abstract rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean"
