    <altURL    rdf:resource="file:vocabularies/rdf-schema.rdf" />
    <language  rdf:resource="http://www.w3.org/2000/01/rdf-schema" />
    <prefix    rdf:datatype="&xsd;string">rdfs</prefix>
</OntologySpec>

